.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  box-sizing: border-box;
} 


.viewName{
  margin-top: 30px;
  margin-left: 40px;
  margin-right: 40px;
  height: 50px;
  font-size: 25px;
  background-color: #EED2EE;

  justify-content: center;
  display: flex;
  align-items: center;
}
.itemView{
  margin-top: 10px;
  margin-left: 10px;
  margin-right: 10px;
  height: 50px;
  
  justify-content: flex-start;
  display: flex;
  align-items: center;
}
